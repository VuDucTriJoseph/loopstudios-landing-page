{
  "name": "loopstudios-landing-page",
  "version": "1.0.0",
  "description": "homepage use only html and css",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "devserver": "live-server --browser=firefox",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style=compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VuDucTriJoseph/loopstudios-landing-page.git"
  },
  "author": "Joseph Vu (Vu Duc Tri)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VuDucTriJoseph/loopstudios-landing-page/issues"
  },
  "homepage": "https://github.com/VuDucTriJoseph/loopstudios-landing-page#readme",
  "dependencies": {
    "live-server": "^1.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "sass": "^1.94.2"
  }
}
